.setting {
  margin-top: 20rpx;
  margin-bottom: 50rpx;
}

.setting-time {
  display: inline-block;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 34rpx;
  font-size: 24rpx;
  padding: 2rpx 24rpx;
  border-radius: 20rpx;
  background: rgba(202, 202, 202, 1);
  margin: 36rpx 0 32rpx;
}

.bottom-text {
  font-weight: 400;
  color: rgba(155, 155, 155, 1);
  line-height: 40rpx;
  font-size: 26rpx;
  margin-right: 12rpx;
}

.bottom-icon {
  width: 14rpx;
  height: 24rpx;
}

.middle-view {
  width: 100%;
  padding-bottom: 14rpx;
  border-bottom: 1rpx solid rgba(155, 155, 155, 0.2);
}

.middle-title {
  display: inline-block;
  width: 630rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 0.85);
  line-height: 42rpx;
  font-size: 28rpx;
}

.middle-text {
  font-weight: 500;
  color: rgba(0, 0, 0, 0.85);
  line-height: 44rpx;
  font-size: 32rpx;
}

.middle-icon {
  width: 12rpx;
  height: 12rpx;
  background: rgba(208, 2, 27, 1);
  border-radius: 50%;
  margin-right: 16rpx;
}

.setting-concent {
  width: 686rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0 0 4rpx 0 rgba(0, 0, 0, 0.12);
  border-radius: 16rpx;
}

.middle-top {
  padding: 24rpx 0 20rpx 24rpx;
}

.bottom-bot {
  padding: 18rpx 40rpx 20rpx 516rpx;
}

/* 缺省页 */

.else-view{
  padding-top: 148rpx;
  border-top: 1rpx solid rgba(155, 155, 155, 0.2)
}

.else-image {
  width: 292rpx;
  height: 280rpx;
}

.else-text {
  font-weight: 400;
  color: rgba(155, 155, 155, 1);
  line-height: 40rpx;
  font-size: 28rpx;
  margin-top: 28rpx
}
