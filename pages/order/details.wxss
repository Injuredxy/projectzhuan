}

.five-icon {
  width: 28rpx;
  height: 28rpx;
}

.five-middle {
  margin-left: 12rpx;
}

.five-right-image {
  width: 36rpx;
  height: 36rpx;
}

.middle-top {
  font-size: 32rpx;
}

.middle-top-title {
  color: #9b9b9b;
