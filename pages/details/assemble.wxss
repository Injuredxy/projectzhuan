/* 店铺推荐 */

.store-view {
  background: #fff;
  margin-top: 16rpx;
}

.store-top-view {
  padding: 16rpx 26rpx 24rpx 26rpx;
}

.store-left-text {
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
  font-size: 32rpx;
}

.store-right-text {
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
  font-size: 24rpx;
}

.store-top-icon {
  width: 24rpx;
  height: 24rpx;
}

.store-views {
  margin: 0 10rpx 32rpx 0;
}

.store-bottom-view {
  padding-left: 20rpx;
  flex-wrap: wrap;
}

.store-image-view {
  position: relative;
  width: 228rpx;
  height: 228rpx;
}

.store-image {
  width: 228rpx;
  height: 228rpx;
  border-radius: 8rpx;
}

.store-image-icon1 {
  position: absolute;
  top: 12rpx;
  right: 16rpx;
  width: 74rpx;
  height: 30rpx;
  background: linear-gradient(135deg, rgba(255, 196, 25, 1) 0%, rgba(252, 155, 5, 1) 100%);
  border-radius: 15rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 26rpx;
  font-size: 18rpx;
}

.store-image-icon2 {
  position: absolute;
  top: 12rpx;
  right: 16rpx;
  width: 74rpx;
  height: 30rpx;
  background: linear-gradient(142deg, rgba(255, 90, 95, 1) 0%, rgba(255, 58, 84, 1) 100%);
  border-radius: 15rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 26rpx;
  font-size: 18rpx;
}

.store-image-icon3 {
  position: absolute;
  top: 12rpx;
  right: 16rpx;
  width: 96rpx;
  height: 30rpx;
  background: rgba(255, 102, 50, 1);
  border-radius: 15rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 26rpx;
  font-size: 18rpx;
}

.store-image-text {
  width: 226rpx;
  height: 80rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
  margin-top: 12rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.store-money-view {
  align-items: baseline;
}

.store-money {
  font-weight: 500;
  color: rgba(248, 82, 78, 1);
  line-height: 38rpx;
  font-size: 27rpx;
  margin-right: 4rpx;
}

.store-price {
  font-weight: 500;
  color: rgba(248, 82, 78, 1);
  line-height: 44rpx;
  font-size: 32rpx;
  margin-right: 8rpx;
}

.store-old-price {
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
  font-size: 24rpx;
  text-decoration: line-through;
}

/* 动画 */

@keyframes slideContentUp {
  from {
    transform: translateY(100%); /*设置为正数则底部弹出来，负数则相反*/
  }

  to {
    transform: translateY(0%);
  }
}

@keyframes slideContentDown {
  from {
    transform: translateY(0%);
  }

  to {
    transform: translateY(100%);
  }
}

